a_alex/Projects/2_EDU/reactjs-course-1120/day-3/src/App.js",["141"],"/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-3/src/hoc/addContainer.js",[],"/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-3/src/redux/rootReducer.js",["142","143","144","145"],"import {genId} from \"../utils\";\n\nconst initialState = {\n    counter: 100,\n    data: [\n        {id: genId(), title: 'Купить хлеба', checked: true, deleted: false},\n        {id: genId(), title: 'Помыть машину', checked: false, deleted: false},\n        {id: genId(), title: 'Изучить ReactJS', checked: true, deleted: false},\n        {id: genId(), title: 'Хорошо поспать', checked: false, deleted: false},\n    ],\n}\n\n// Reducer\nconst rootReducer = (state = initialState, action) => {\n    switch (action.type){\n        case 'ADD':\n            return {...state, counter: state.counter + 1 }\n            break\n        case 'SUB':\n            return {...state, counter: state.counter - 1 }\n            break\n        case 'DELETE_TASK':\n            console.log(action, state)\n            const task = state.data.find(el => el.id === action.value)\n            task.deleted = !task.deleted\n            const data = [...state.data]\n            return {...state, data}\n            break\n    }\n    return state\n}\n\n\nexport default rootReducer","/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-3/src/redux/reducers/counter.js",["146","147","148"],"/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-3/src/redux/reducers/tasklist.js",["149","150"],"/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-3/src/redux/actions/actionList.js",[],"/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-4/src/redux/rootReducer.js",[],["151","152"],"/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-4/src/components/NewTask/NewTask.js",[],"/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-4/src/redux/actions/actionList.js",[],"/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-4/src/App.js",[],"/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-4/src/pages/TaskListPage.js",[],"/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-4/src/pages/HomePage.js",[],"/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-4/src/components/TaskList/TaskList.js",["153"],"import React from 'react'\nimport Task from \"../Task/Task\";\n\nimport {connect} from \"react-redux\";\nimport {Route,withRouter} from \"react-router-dom\";\n\n\n    let  TaskAbout = (props) => {\n        console.log('TaskAbout', props)\n        return <div>Info</div>\n    }\n\n\nTaskAbout = withRouter(TaskAbout)\n\nconst TaskList = (props) => {\n    console.log('TaskList', props);\n    const tasklist = props.data\n                        .filter(task => props.filter.deleted === task.deleted)\n                        .filter(task => props.filter.checked === task.checked)\n                        .map(task => (\n        <Task\n            key={task.id}\n            task={task}\n        />\n    ))\n    return (\n        <div>\n            <h3>Список задач</h3>\n            <button\n                className='btn btn-primary'\n                onClick={() => props.changeFilter({deleted: !props.filter.deleted})}>\n                 { !props.filter.deleted? 'Показать': 'Скрыть'} удаленные\n            </button>\n            <button className='btn btn-primary' onClick={() => props.changeFilter({checked: !props.filter.checked})}>Показать заверженные</button>\n            {tasklist}\n\n            { true &&  <TaskAbout />}\n        </div>\n    )}\n\nfunction mapStateToProps(state){\n    return {data: state.tasklist.data}\n}\n\n\n\nexport default  connect(mapStateToProps)(TaskList)","/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-4/src/components/Nav/Nav.js",[],"/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-4/src/pages/About.js",[],"/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-4/src/redux/reducers/counter.js",["154","155","156"],"import {ADD, SUB} from \"../actions/actionTypes\";\n\nconst initialState = {\n    counter: 100,\n}\n\n// Reducer\nconst counter = (state = initialState, action) => {\n    switch (action.type){\n        case ADD:\n            return {...state, counter: state.counter + 1 }\n            break\n        case SUB:\n            return {...state, counter: state.counter - 1 }\n            break\n    }\n    return state\n}\n\n\nexport default counter","/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-4/src/redux/actions/actionTypes.js",[],"/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-4/src/components/Task/Task.js",["157"],"import React from 'react'\nimport {DataContext} from \"../../App\";\nimport {connect} from \"react-redux\";\nimport {del, del_async} from \"../../redux/actions/actionList\";\n\n\nconst Task = ({task, onClickAction, deleteTask, checkTask}) => {\n    const inputText = React.createRef()\n\n    const handlerClick = (event) => {\n        if (inputText.current.disabled === false){\n            event.target.innerText = 'Редактировать'\n            onClickAction(inputText.current.value)\n        } else {\n            event.target.innerText = 'Сохранить'\n        }\n        inputText.current.disabled = !inputText.current.disabled\n\n    }\n\n    return (\n            <div className={task.deleted?'input-group mb-3 ml-4': 'input-group mb-3'}>\n                <div className=\"input-group-prepend\">\n                    <div className=\"input-group-text\">\n                        <input type=\"checkbox\" defaultChecked={task.checked}  onChange={() => checkTask(task.id)}/>\n                    </div>\n                </div>\n            <input type=\"text\" className=\"form-control\" ref={inputText} defaultValue={task.title} disabled={true}/>\n            <div className=\"input-group-append\">\n                <button onClick={handlerClick} className=\"btn btn-outline-secondary\">Редактировать</button>\n                <button className=\"btn btn-outline-secondary\" onClick={() => deleteTask(task.id)}>Удалить</button>\n            </div>\n            </div>\n    )\n}\n\n\n\n\n\nfunction mapDispatchToProps(dispatch){\n    return {\n        deleteTask: (id) => dispatch(del(id)),\n        checkTask: (id) => dispatch(del_async(id))\n    }\n}\n\nexport default connect(null, mapDispatchToProps)(Task);\n\n","/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-4/src/redux/reducers/tasklist.js",["158","159","160","161"],"import {genId} from \"../../utils\";\nimport {DELETE_TASK, NEW_TASK, NEW_TASK_LIST} from \"../actions/actionTypes\";\n\nconst initialState = {\n    data: [\n        {id: genId(), title: 'Купить хлеба', checked: true, deleted: false},\n        {id: genId(), title: 'Помыть машину', checked: false, deleted: false},\n        {id: genId(), title: 'Изучить ReactJS', checked: true, deleted: false},\n        {id: genId(), title: 'Хорошо поспать', checked: false, deleted: false},\n    ],\n}\n\n// Reducer\nconst tasklist = (state = initialState, action) => {\n    let task = null\n    let data = null\n    switch (action.type){\n        case DELETE_TASK:\n            console.log(action, state)\n            task = state.data.find(el => el.id === action.value)\n            task.deleted = !task.deleted\n            data = [...state.data]\n            return {...state, data}\n            break;\n        case NEW_TASK:\n            task = {id: genId(), title: action.value, checked: false, deleted: false}\n            return {data: [...state.data, task]}\n            break;\n        case NEW_TASK_LIST:\n            return {data: [...state.data, ...action.value]}\n            break;\n\n    }\n    return state\n}\n\n\nexport default tasklist","/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-4/src/index.js",[],"/Users/lea_alex/Projects/2_EDU/reactjs-course-1120/day-4/src/utils/index.js",[],{"ruleId":"162","severity":1,"message":"163","line":56,"column":11,"nodeType":"164","messageId":"165","endLine":56,"endColumn":19},{"ruleId":"162","severity":1,"message":"166","li